    <body style="background-image: url('/image/wep-bg1.jpg'); background-size: cover; background-position: fixed; background-repeat: no-repeat; background-attachment: fixed; background-position: center;">
        
        
        <nav class=" nav_bar fixed-top" >

            <a class="navbar-brand " href="/dinsor" >
                <center><img style="filter: invert(100%);" src="/image/Dinsor_brand.png" width="40" height="40" >
                <span class="prof">DinSor</span></center>
            </a>

            <form class=" search_bar" action="/dinsor/search" method="GET">
                <input type="search" name="search" placeholder="Search..." aria-label="Search">
                <button type="submit" formaction="/dinsor/search"><i class="fa fa-search"></i></button>
            </form>
            
            <div class="hidden">
                <i type="button" class="dropdown-toggle-split fas fa-search" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></i>
                <div class="dropdown-menu">
                    <div class="dropdown-item search_bar">
                        <form  action="/dinsor/search" method="GET">
                            <input type="search" name="search" placeholder="Search..." aria-label="Search">
                            <button type="submit" formaction="/dinsor/search"><i class="fa fa-search"></i></button>
                        </form>
                    </div>
                </div>
            </div>

            <form action="/Index" method="POST" class="user_icon">
                <% if(!currentUser){ %>
                    <div class="btn-group col-lg-3">
                        <text class="no_edge_btn">Guest</text> 
                        <button type="button" class="no_edge_btn dropdown-toggle dropdown-toggle-split" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="sr-only">Toggle Dropdown</span>
                        </button>
                        <div class="dropdown-menu">
                            <a class="dropdown-item" href="/login">Login</a>
                            <a class="dropdown-item" href="/register">Register</a>
                        </div>
                    </div>
                <% }else{ %>
                    <div class="btn-group col-lg-3">
                        <text class="no_edge_btn"><%= currentUser.username %></text> 
                        <button type="button" class="no_edge_btn dropdown-toggle dropdown-toggle-split" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="sr-only">Toggle Dropdown</span>
                        </button>
                        <div class="dropdown-menu">
                            <!-- <a class="dropdown-item" href="/editprofile">Edit Profile</a>
                            <div class="dropdown-divider"></div> -->
                            <a class="dropdown-item" href="/logout">Logout</a>
                        </div>                        
                    </div>
                <% } %>

            </form>
            <form action="/Index" method="POST" class="hidden">
            <% if(!currentUser){ %>
                <i type="button" class="dropdown-toggle-split fas fa-user-circle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></i>
                <div class="dropdown-menu">
                    <a class="dropdown-item" href="/login">Login</a>
                    <a class="dropdown-item" href="/register">Register</a>
                </div>
            <% }else{ %>
                <i type="button" class="dropdown-toggle-split fas fa-user-circle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></i>
                <div class="dropdown-menu">
                    <!-- <a class="dropdown-item" href="/edit">Edit Profile</a>
                    <div class="dropdown-divider"></div> -->
                    <a class="dropdown-item" href="/logout">Logout</a>
                </div> 
            <% } %>
            </form>
            <form action="/Index" method="POST" class="hidden">
            <i type="button" class="dropdown-toggle-split fas fa-book" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></i>
                <div class="dropdown-menu">
                    <a class="dropdown-item" style="text-decoration: none;" href="/dinsor/CSS_224">CSS 224</a>
                    <a class="dropdown-item" style="text-decoration: none;" href="/dinsor/CSS_226">CSS 226</a>
                    <a class="dropdown-item" style="text-decoration: none;" href="/dinsor/CSS_227">CSS 227</a>
                    <a class="dropdown-item" style="text-decoration: none;" href="/dinsor/CSS_228">CSS 228</a>
                    <a class="dropdown-item" style="text-decoration: none;" href="/dinsor/LNG_224">LNG 224</a>
                    <a class="dropdown-item" style="text-decoration: none;" href="/dinsor/GEN_241">GEN 241</a>
                </div>
            </form>
