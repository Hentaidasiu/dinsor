<%- include("./partials/header") %>
<div class="content">
    <div class="post_box">
        <form action="/dinsor/CSS_224" method="POST">
            <div class="form-group">
                <h5>Create new post here.</h5><br>
                <input type="text" name="text" class="form-control">
            </div>
            <button class="btn btn-warning" type="submit">Post</button>
        </form>
    </div>
    <% css224DB.reverse().forEach(function(css224DB){ %>
        <div class="post_box">
            <p>Post by : <%= css224DB.p_username %></p><br>
            <p><%= css224DB.title %></p>
            <img src=<%= css224DB.image %>>
        </div>
    <% }); %>
</div>
<%- include("./partials/footer") %>