<%- include("partials/header") %>
<%- include("partials/headest") %>
    <div style="margin-left: 180px; margin-top: 10vh;">
        <div style="width: 90%; background-color: rgba(255, 255, 255, 0.9); border-radius: 10px; margin: 1cm;">
            <form action="/dinsor/css_224" method="POST">
                <div class="form-group">
                    <label for="exampleInputEmail1"><strong>Create new post here.</strong></label>
                    <input type="text" name="text" class="form-control">
                </div>
                <button class="btn btn-warning" type="submit">Add</button>
            </form>
        </div>
        <% css224DB.reverse().forEach(function(css224DB){ %>
            <div style="width: 90%; background-color: rgba(255, 255, 255, 0.9); border-radius: 10px; margin: 1cm;">
                <p>Post by : <%= css224DB.p_username %></p><br>
                <p><%= css224DB.title %></p>
                <img src=<%= css224DB.image %>>
                <a class="btn btn-warning" href="/dinsor/<%= css224DB._id %>">More detail...</a>
            </div>
        <% }); %>
    </div>
<%- include("partials/footer") %>