<%- include("./partials/header") %>
<div class="content">
    <div class="post_box" style="text-align: center;">
        <h4>Welcome to CSS224 group</h4>
    </div>
    <div class="post_box">
        <form action="/dinsor/CSS_224" method="POST">
            <div class="form-group">
                <h5>Create new post here.</h5><br>
                <input type="text" name="text" class="form-control">
            </div>
            <button class="btn btn-warning" type="submit">Post</button>
        </form>
    </div>
    <% css224DB.forEach(function(css224DB){ %>
        <div class="post_box">
            <p class="headpostfont">Post by : <%= css224DB.owner_id.username %></p>
            <p><%= css224DB.title %></p>
            <img src="/image/Dinsor_brand.png">
            <hr>
            <a href="/dinsor/<%= css224DB._id %>" class="btn btn-warning">More detail</a>
        </div>
    <% }); %>
</div>
<%- include("./partials/footer") %>